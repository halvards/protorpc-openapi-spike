[flake8]
max-line-length = 80
import-order-style=google
application-import-names=google
exclude =
  __pycache__,
  .git/,
  *.pyc,
  conf.py,
  .env/,
  env/,
  env.bak/,
  .venv/,
  venv/,
  venv.bak/
